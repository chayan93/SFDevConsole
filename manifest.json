{
  "manifest_version": 2,
  "name": "Open Debug Log",
  "version": "0.1",
  "background": {
    "scripts": ["background.js"]
  },
  "content_scripts": [
    {
      "matches": [
				"https://*.force.com/*",
				"https://*.salesforce.com/*"
			],
      "js": ["content.js"]
    }
  ],
  "browser_action": {
    "default_icon": "icon.png"
  }
}
